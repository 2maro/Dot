# ANSI Color Tests
.ANSI-30-black 30                   # Black
.ANSI-01;30-brblack 01;30    [I       # Bright black (gray)
.ANSI-31-red 31                     # Red
.ANSI-01;31-brred 01;31             # Bright red
.ANSI-32-green 32                   # Green
.ANSI-01;32-brgreen 01;32           # Bright green
.ANSI-33-yellow 33                  # Yellow
.ANSI-01;33-bryellow 01;33          # Bright yellow
.ANSI-34-blue 34                    # Blue
.ANSI-01;34-brblue 01;34            # Bright blue
.ANSI-35-magenta 35                 # Magenta
.ANSI-01;35-brmagenta 01;35         # Bright magenta
.ANSI-36-cyan 36                    # Cyan
.ANSI-01;36-brcyan 01;36            # Bright cyan
.ANSI-37-white 37                   # White
.ANSI-01;37-brwhite 01;37           # Bright white

